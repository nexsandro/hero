alter table TB_TERM drop constraint FKD18D3F9D31E19234;
alter table menu drop constraint FK33155F8AFE91F3;
alter table menu_role drop constraint FKA4FEA756AD22C6A4;
alter table menu_role drop constraint FKA4FEA756AD1DF076;
alter table tb_user drop constraint FKA4FF6AFCEF93AF86;
alter table user_role drop constraint FK143BF46A8B04B0E2;
alter table user_role drop constraint FK143BF46A8B01DA38;
drop table LOCK;
drop table TB_MDIC_KEYW;
drop table TB_TERM;
drop table menu;
drop table menu_role;
drop table role;
drop table tb_tag;
drop table tb_user;
drop table user_role;
create table LOCK (cd_lock varchar(255) not null unique, primary key (cd_lock));
create table TB_MDIC_KEYW (SQ_KEYW int identity not null, TX_DESC varchar(255) null, CD_KEY varchar(255) not null, NU_VERS int not null, primary key (SQ_KEYW));
create table TB_TERM (SQ_TERM int identity not null, tx_text varchar(255) null, SQ_KEYW int null, primary key (SQ_TERM));
create table menu (cd_menu int not null, no_menu varchar(255) not null, cd_prnt int null, primary key (cd_menu));
create table menu_role (cx_menu int not null, cx_role varchar(255) not null, primary key (cx_menu, cx_role));
create table role (cd_role varchar(255) not null, no_role varchar(255) null, primary key (cd_role));
create table tb_tag (cd_tag int identity not null unique, no_tag varchar(255) not null, primary key (cd_tag));
create table tb_user (cd_user varchar(255) not null, no_user varchar(255) null, nu_vers int null, sq_mngr varchar(255) null, primary key (cd_user));
create table user_role (cd_user varchar(255) not null, cd_role varchar(255) not null, primary key (cd_user, cd_role));
alter table TB_TERM add constraint FKD18D3F9D31E19234 foreign key (SQ_KEYW) references TB_MDIC_KEYW;
alter table menu add constraint FK33155F8AFE91F3 foreign key (cd_prnt) references menu;
alter table menu_role add constraint FKA4FEA756AD22C6A4 foreign key (cx_role) references role;
alter table menu_role add constraint FKA4FEA756AD1DF076 foreign key (cx_menu) references menu;
alter table tb_user add constraint FKA4FF6AFCEF93AF86 foreign key (sq_mngr) references tb_user;
alter table user_role add constraint FK143BF46A8B04B0E2 foreign key (cd_user) references tb_user;
alter table user_role add constraint FK143BF46A8B01DA38 foreign key (cd_role) references role;
